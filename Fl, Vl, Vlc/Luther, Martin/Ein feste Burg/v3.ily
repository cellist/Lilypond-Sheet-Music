vc = \relative c {
  \voiceconsts
  \clef "bass"
  
  \repeat volta 2 {
    \partial 2 <g d'>2\upbow\mf
    h <g d'>
    d' h
    e a
    d,\fermata e
    h' c
    g gis
    a d,
    g?\fermata
  }

  h,
  fis' e
  h' ais
  h,\fermata h'\upbow
  fis g4( fis)
  g2 dis
  e\fermata h'
  e, h'
  g( a?)

  d,\fermata c\upbow
  c g'
  e a
  h\fermata e,
  h' c
  g e
  a d,
  \partial 2 <g, d'> \bar "|."
}