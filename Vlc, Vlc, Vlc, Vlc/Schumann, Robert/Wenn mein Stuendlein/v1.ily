va = \relative c'' {
  \voiceconsts
  \clef "tenor"

  \repeat volta 2 {
    \partial 2 g2
    d e4( fis) g2 g
    a h g\fermata h4( c)
    d2 d4( c) h2 d

    c1 h2\fermata
  }
  h
  c h a g
  a h4( a) g2\fermata a

  h h e, fis
  g4( fis) e2 d\fermata d
  g a h c4( h)
  \partial 1. a1 g2\fermata \bar "|."
}