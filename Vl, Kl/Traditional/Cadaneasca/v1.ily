va = \relative c'' {
  \voiceconsts

  \repeat volta 2 {
    a8\f b cis d e a, f' e d
    cis e d cis b a e' e a,
    a b cis d e a, f' e d
    cis e d cis b a a a g
  }

  \repeat volta 2 {
    d'4 d d f8 e d
    cis4 cis cis e8 cis b
    b4 b b d8 b a
    cis[ e d] cis[ b g] a[ a e]
  }

  a8\f b cis d e a, f' e d
  cis e d cis b a e' e a,
  a b cis d e a, f' e d
  cis e d cis b a \breathe a4. \bar "|."
}