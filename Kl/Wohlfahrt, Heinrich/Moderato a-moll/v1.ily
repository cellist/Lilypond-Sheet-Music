va = \relative c' {
  \voiceconsts

  \repeat volta 2 {
    \partial 4. e8( a h)
    c4. a8( c d)
    e4. c8( h a)
    gis4. f8( e gis)
    a4 r8 e( a h)
    c4. a8( c d)

    e4. c8( h a)
    gis4. f8( e gis)
    a4 r8
  }

  \repeat volta 2 {
    e( a c)
    h4. e,8( gis h)
    a4. e8( a c)

    c4( h8)-. r a r
    gis4 r8 e( a h)
    c4. a8( c d)
    e4. c8( h a)
    gis4. f8( e gis)
    a4 r8
  }
}