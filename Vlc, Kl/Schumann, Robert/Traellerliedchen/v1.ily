va = \relative c' {
  \voiceconsts
  \clef "tenor"
  
  cis4(\p h) a( h)
  cis( d) e2
  d4( e) cis( d)
  h( d) a( h)

  cis(\p h) a( h)
  cis( d) e2
  d4( e) cis( d)
  h2( a4) r \bar "||"

  gis'(\downbow\mf fis) e( fis)
  gis( a) h2
  a4( h) gis( a)
  fis( a) e( fis)

  gis(\downbow fis) e( fis)
  gis(\< a) h( cis)
  a(\!\f h) gis( a)
  fis2( e8) gis,(\> a h) \bar "||"

  cis4(\!\p h) a( h)
  cis( d) e2
  d4( e) cis( d)
  h( d) a( h)

  cis( h) a( h)
  cis( d) e( fis)
  d( e) cis( d)
  h2( a4) r \bar "|."
}