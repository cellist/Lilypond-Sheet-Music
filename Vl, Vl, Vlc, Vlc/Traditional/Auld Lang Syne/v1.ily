va = \relative c' {
  \voiceconsts

  \repeat volta 2 {
    \partial 4 c4
    f4. f8 f4 a
    g4. f8 g4 a
    f4. f8 a4 c
    d2. f4

    c4. a8 a4 f
    g4. f8 g4 a
    f4. d8 d4( c)
    f2. \breathe d'4

    c8( a4.) a4( f)
    g4. f8 g4 d'
    c8( a4.) a4( c)
    d2. f4

    c4. a8 a4 f
    g4. f8 g4 a8 g
    f( d4.) d4( c)
    f2.
  }
