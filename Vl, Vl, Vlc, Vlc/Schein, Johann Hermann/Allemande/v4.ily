vd = \relative c {
  \voiceconsts
  \clef "bass"
  
  \repeat volta 2 {
    e4. e8 a4 fis
    e2 e
    a,4 h c2
    h2. h4
    cis? dis e cis
    h2. cis4
    dis e h2
    e1
  }

  \repeat volta 2 {
    h4. h8 a4 d?
    g,2. d'4

    e h cis2
    h2. g4
    a h c d
    g,1
  }

  \repeat volta 2 {
    g'4. g8 d4 d
    e2 h
    h'4. h8 fis4 fis
    g2 fis
    d4. d8 a4 a
    h2. cis4
    dis e h2

    e1
  }
