vc = \relative c' {
  \voiceconsts
  \clef "bass"
  
  \repeat volta 2 {
    e4. e8 e4 fis
    gis2 e
    e4 d c8 d e4
    dis2. dis4
    e h h e,
    fis2. e4
    fis g fis2
    gis1
  }

  \repeat volta 2 {
    h4. h8 c4 a

    h4. a8 g?4 a
    h h2 a4
    h4. cis?8 dis4 h
    a g8 fis e4 a
    h1
  }

  \repeat volta 2 {
    d?4. d8 d4 a
    g8 a h4 h2
    fis'4. fis8 fis4 cis
    h2 cis

    d4. d8 cis4 e
    d2 d,4 e
    fis g fis2
    gis1
  }
}