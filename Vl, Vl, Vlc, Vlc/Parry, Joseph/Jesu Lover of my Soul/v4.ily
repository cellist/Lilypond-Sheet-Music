vd = \relative c' {
  \voiceconsts
  \clef "bass"
  
  \repeat volta 2 {
    g2 b4( d,) g2 b4( c)
    d2 d, g1
    g2 a b c
    b4( a) g2 d1
    g4( a) b( a) g2 b4( c)
    d,2 d g1
    d2 g4( f) b2 c

    d d, g1
    es2 b' es,4( d) c'2
    b f b1
    g2 d g f
    b a d,1
    es2 es' d b
    g es d1
    b'4( c) b( a) g2 b4( c)

    d,2 d g1
  }
}