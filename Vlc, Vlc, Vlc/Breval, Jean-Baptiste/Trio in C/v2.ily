vb = \relative c' {
  \voiceconsts
  \clef "tenor"
  
  \introa
  \repeat volta 2 {
    \partial 4 c4^\solo
    g'2 f8(\trill e f g)
    f( e) e4~ \tuplet 3/2 4 { e8 c d e[ f g] }
    g( f) e d c4 h\trill
    d2( c4) r
    a'4. g8 g( f) e f

    g2( e8) c e g
    a4. g8 g( f) e f
    g2( e4) r
    g8( fis) fis2 e4
    \appoggiatura fis16 e8( d) d2 c4

    h4. c8 d( h c a)
    h4. c8 d e fis g
    g( e) e2.
    \tuplet 3/2 4 {
      g8( fis e) e([-. e-. e)]-. e( d c) c([-. c-. c])-.
      e( d c) c([-. c-. c])-. c( h a) a([-. a-. a])-.

      h g a h[ c d] e d c h[ a g]
      fis d' a fis[ a fis]
    } d2
    d' h8( d c e)
    d2. d4
    e g( fis e)

    e8( d cis d) d2
    g,4. a8 h4 h
    h4.( c8) a4 r
    a4. h8 c4 c
    c4.( d8) h4 r

    d4.( e16 fis) g4 g
    h,4.( c16 d) e4 e
    e8( d) c h d( c) h a
    d4.( e16 fis) \repeat tremolo 4 g8
    h,4.( c16 d) \repeat tremolo 4 e8

    d g fis e d c h a
    \tuplet 3/2 4 {
      g( h) d-. a[( c) d]-. g,( h) d-. a[( c) d]-.
      g, h d g[ fis e] d e d c[ h a]
      g( h) d-. a[( c) d]-. g,( h) d-. a[( c) d]-.
    }

    h16 g a h c[ d e fis] g fis e d c[ h a g]
    e' fis g fis e[ fis g fis] e fis g fis e[ d c h]
    a h c h a[ h c h] a h c h a[ g fis e]
    d1

    a'\trill
    g4 h8 d d( c) a c
    c( h) d h d( c) a fis
    fis( g) h d d( c) a c
    c( h) d h d( c) a fis
    g4 \clef "bass" <g, h' g'> <g h' g'>
  }
  \repeat volta 2 {
    g' \clef "tenor"
    d'2 c8(\trill h c d)
    c( h) h2 h8 d
    d( c) h a g4 fis\trill
    fis2( g4) r
    d'4. c8 c( h) a g

    g c c4~ c8 h( c cis)
    d4. c!8 c( h) a g
    c\trill c16 h c8[ d] e2
    f?4. e8 d d( e f)
    e4. d8 c c( d e)

    e( d f) e d c h a
    gis16 e' d e f[ e d e] c,2
    a'4. h8 c d e f
    e( d) d2.
    d4. f8 e( d c h)

    d( c) c2.
    e2 a
    a,2. h8 cis
    d( e f d) c!( h e d)
    d2( c4) r

    e2 a
    a,2. h8 c
    c( h) h2 a4
    \tuplet 3/2 4 {
      gis8 e e e[ e e] e gis a h[ c h]
      a e e e[ e e] e a h c[ d c]

      h e, e e[ e e] e h' c d[ c h]
      c a a d[ a a] e' a, a f'[ a, a]
      e' a, a fis'[ a, a] gis' a, a a'[ a, a]
    }
    e1
    h'\trill

    a2 r
    e'~ e8 d c h
    h(\trill a) a4 r2
    f'?~ f8 e d c
    h c d c h a g? f

    e g c4~ c8 cis d4~
    d8 e f4~ f8 e( f fis)
    g2 f!8(\trill e f g)
    f( e) e4~ \tuplet 3/2 4 { e8 c? d e[ f g] }
    g( f) e d c4 h\trill

    d2( c4) r
    a'4. g8 g( f) e f
    g2( e8) c e g
    a4. g8 g( f) e f
    g2( e4) r

    \tuplet 3/2 4 {
      g8 e f g[ e f] g f e d[ c h]
      a( cis d) d[(-. d-. d)]-. d( e f) f[(-. f-. f)]-.
      a g f e[ d cis] d e f e[ d c] \clef "bass"
      h g fis g[ d h]
    } g2

    g' e8( g f a)
    g2. g4
    a( c h a)
    a8( g fis g) g2 \clef "tenor"
    c4. d8 e4 e
    e4.( f?8) d4 r

    d4. e8 f4 f
    f4.( g8) e4 r
    g4.( a16 h) c4 c
    e,4.( f16 g) a4 a
    a8( g) f e g( f) e d
    g4.( a16 h) \repeat tremolo 4 c8

    e,4.( f16 g) \repeat tremolo 4 a8
    g c h a g f e d
    \tuplet 3/2 4 {
      c( e) g-. d[( f) g]-. c,( e) g-. d[( f) g]-.
      c, e g c[ h a] g a g f[ e d]
      c( e) g-. d[( f) g]-. c,( e) g-. d[( f) g]-.
    }

    e16 c d e f[ g a h] c h a g f[ e d c]
    a' h c h a[ h c h] a h c h a[ g f e]
    d e f e d[ e f e] d e f e d[ c h a]
    g1

    d'\trill
    c4 e8 g g( f) d f
    f( e) g e g( f) d h
    c4 e8 g g( f) d f
    f( e) g e g( f) d h
    \partial 2. c4 \clef "bass" <g, e' c'> <g e' c'>
  }  
}