vc = \relative c {
  \voiceconsts
  
  \repeat volta 2 {
    f2\mf e4( f)
    b4.( f8) f2
    a f4(-- f)--
    c1
    f2\mp e4 f
    b4.( f8) f2
    c c4(-- c)--
    f,1 \breathe

    f'2\mf b,4 f'
    f4.( a8) f2
    a\mp d,4( f)
    a4.( f8) e2
    f e4( f)
    b4.( f8) f2
    c c4(-- c)--
    f,2. r4
  }
}