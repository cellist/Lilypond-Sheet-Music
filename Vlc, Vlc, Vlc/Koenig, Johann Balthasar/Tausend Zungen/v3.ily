vc = \relative c {
  \voiceconsts

  f2. f4
  b f d f
  r c' a c
  d c b a
  g2 f4 a
  g d c2
  f1 \breathe

  \repeat volta 2 {
    r4 a f a
    b a g f
    c'2 a4 \breathe f
    g d c f
    b, f' c2

    r4 e f g8 a
    b4. a8 g4 f
    b, c d \breathe a
    b d c f8 e
    d4 f c2

    r4 c b c
    f4. e8 d4 g
    c,2 r4 a'8 g
    f e d c b c d e
    f4 c f2
  }
}