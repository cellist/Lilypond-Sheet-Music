vc = \relative c {
  \voiceconsts

  \repeat volta 2 {
    d2. b4
    g a b b'16( a g f)
    e4 d cis e
    cis a d, f8 a
    d2. b4
    g a b2

    a4 d g, f
    b c,? f2
  }

  \repeat volta 2 {
    a4 b8 g a4 f
    f' f,2 g16( f e d)
    g4 f e a
    f g c, r

    c' d8 h c4 d16( c h a)
    d4 f h,2
    c4 e, f c
    d e a r
    fis2. g4
    a d, b'? c16( b a g)

    cis2. cis4
    d b a r
    d2. r4
    r c? b8 a g f
    e f e d cis4 d
    g a d,2
  }
