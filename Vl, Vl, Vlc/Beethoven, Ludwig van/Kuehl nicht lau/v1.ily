va = \relative c' {
  \voiceconsts

  \partial 2. b( a) c
  h2. h4
  c g'( as) f
  g ges( f) c'
  b? r r2

  r4 g?( f) as
  g es'( d) f
  es2. es4
  d
  \repeat volta 2 {
    r r2
    r4 es,( d) f

    es c( h) d
    c b!( a?) f
    b b( a) c
    h2. h4
    c g'( as) f

    g ges( f) c'
    b? r r2
    r4 g?( f) as
    g es'( d) f
    es2. es4
    d
  }
}