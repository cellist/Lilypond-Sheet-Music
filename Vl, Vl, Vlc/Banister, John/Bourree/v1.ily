va = \relative c'' {
  \voiceconsts

  \repeat volta 2 {
    b4 b8 as g as b4
    es d es f,
    g8 as b g c4 f,
    f2 es
  }

  \repeat volta 2 {
    f'4 f8 es d es f d

    g f es d c d es c
    f d g f es d es c
    c2 b4 b8 as
    g b es, g as c f, as
    b d g, b c es as, c

    d f b, g c b as g
    f2 es
  }
