vc = \relative c' {
  \voiceconsts
  \clef "bass"
  
  \repeat volta 2 {
    h2. d4 c2
    d a4 h2 d4
    d h4. c8 d4 e c
    d2 e4 h2 d4
    g,2 g h4 c
    d2. d4 a2

    g a d
    d1.
  }

  \repeat volta 2 {
    d2 h4 d h g
    g c a h2 h4
    a1 a2
    a h4 d a h
    h2 d d

    c g'4 f8 e d2
    d e a,
    a1.
  }

  \repeat volta 2 {
    h2. d4 c d
    h2 a4 g2 d'4
    h2. h4 c a

    e'2 a,8 h cis2 d4
    h2 d4 e c! h
    h2 d4 g,2 g4
    g2 d'4 h a2
    g1.
  }
