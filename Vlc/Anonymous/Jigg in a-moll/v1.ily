va = \relative c' {
  \voiceconsts

  \repeat volta 2 {
    a4.\upbow h\downbow
    c8\upbow h a c\downbow h a
    h4. c
    d8\upbow c h d\downbow c h
    c4 c8 a\upbow h c\downbow
    d4 d8 h c d\downbow
    c h a gis a4
    h e,8 e fis gis\downbow
  }

  \repeat volta 2 {
    a4. cis
    d4 d,8 f?\upbow e d\downbow
    g?4. h
    c4 c,8 e d c\downbow
    a'4. h
    c8 h a\downbow h c d
    c h a e4 gis8
    a4. a,
  }
}