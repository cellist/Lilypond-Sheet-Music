vc = \relative c' {
  \voiceconsts
  \clef "tenor"

  \repeat volta 2 {
    d4 d d4. d8
    d d c h16 a g4. d'8
    c4 c d d
    g d8 cis fis8. g16 fis8. e16
    d4 fis d8. h16 cis4
    a d d2
  }

  \repeat volta 2 {
    a'8 g fis e fis8. g16 a4

    fis8 e fis16 g a8 d, e16 fis g4
    g d h g'
    fis8 d e8. e16 d2
    a8 d d e a,8. a16 a4
    h8 d c8. c16 g'2
    a4 a, a8 d c c
    h8. c16 d4 d2
  }
}