vd = \relative c' {
  \voiceconsts
  \clef "bass"

  \repeat volta 2 {
    h4 h h4. h8
    h h a d c4. g'8
    g4 fis h, a
    h8 g a8. cis16 d4 a
    h d h r8 a
    fis4 a a2
  }

  \repeat volta 2 {
    fis'8 e d g, a8. a16 a4

    d8 cis d a h8. c16 d4
    h8 g a4 g g
    a a8. a16 a2
    fis'8 h, fis' g a,8. g'16 fis4
    g,8 g e'8. a,16 h2
    d4 e d8 g, g' fis
    d4 h8. a16 h2
  }
}