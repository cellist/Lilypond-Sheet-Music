va = \relative c'' {
  \voiceconsts

  \repeat volta 2 {
    d8. es16 d es f8 c f
    b,8. c16 b c d8 a d
    g,8. a16 g a b8 f d'
    b4.\downprall a8 b4
  }

  \repeat volta 2 {
    b'8. a16 g a b8 a g
    fis8. g16 fis g a8 d, a'
    b8. a16 g a b8 a g
    g4.\downprall fis8 g4

    g,8. a16 g a b8 a16 b c8
    b8. c16 b c d8 c16 d es8
    d8. es16 d es f8 f, f'
    b,4.\downprall c16 d b4
  }
}